<% if documentables.present? %>
  <div class="node is_group">
    <span class="node__self">
      <span class="node__title">
        <%= title %>
      </span>
    </span>

    <% documentables.each do |feature| %>
      <%= partial 'api/nodes/documentable', locals: { documentable: feature } %>
    <% end %>
  </div>
<% end %>
